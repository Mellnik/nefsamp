	    }
	    
	    if(count_r > m_PlayerRecord) {
	        m_PlayerRecord = count_r;
	        server_save_config();
	    }
	
	    InitSession(playerid);
	
		TextDrawShowForPlayer(playerid, TXTOnJoin[0]);
		TextDrawShowForPlayer(playerid, TXTOnJoin[1]);

