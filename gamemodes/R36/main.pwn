	    }
	    case ACCOUNT_REQUEST_GANG_LOAD:
	    {
		    if(cache_get_row_count() > 0)
			{
		        cache_get_row(0, 0, PlayerData[playerid][GangName], pSQL, 25);
		        cache_get_row(0, 1, PlayerData[playerid][GangTag], pSQL, 5);

