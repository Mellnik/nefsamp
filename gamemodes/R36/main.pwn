	    {
	        cache_get_row(i, 0, tmp, pSQL, sizeof(tmp));
	        cache_get_row(i, 1, tmp2, pSQL, sizeof(tmp2));
	        new col = cache_get_row_int(i, 3, pSQL);
	        if(col != 0)
	        {
	        	format(gstr, sizeof(gstr), "{%06x}%i - [%s]%s [%i]\n", col >>> 8, i + 1, tmp2, tmp, cache_get_row_int(i, 2, pSQL));
			}
