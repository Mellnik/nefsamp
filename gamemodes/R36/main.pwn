YCMD:l(playerid, params[], help)
{
    if(PlayerData[playerid][bGWarMode]) return SCM(playerid, -1, ""er"You can't use this command in Gang War mode, use /exit");
	if(gTeam[playerid] != FREEROAM) return SCM(playerid, RED, NOT_AVAIL);
