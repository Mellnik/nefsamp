					        PlayerData[i][e_gangid] = 0;
					        PlayerData[i][TmpGangID] = 0;
					        PlayerData[i][e_gangrank] = GANG_POS_NONE;
					        PlayerData[i][GangName][0] = '\0';
							PlayerData[i][GangTag][0] = '\0';
