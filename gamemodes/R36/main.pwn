badsql(const string[], bool:strict = true)
{
	if(strict)
	{
	    if(strfind(string, " ", true) != -1) return 1;
	    if(strfind(string, ",", true) != -1) return 1;
	}

	if(strfind(string, "|", true) != -1) return 1;
	if(strfind(string, "@", true) != -1) return 1;
	if(strfind(string, "*", true) != -1) return 1;
	if(strfind(string, "'", true) != -1) return 1;
	if(strfind(string, "/", true) != -1) return 1;
	if(strfind(string, "\\", true) != -1) return 1;
	if(strfind(string, "´", true) != -1) return 1;
	if(strfind(string, "`", true) != -1) return 1;
	if(strfind(string, "~", true) != -1) return 1;
	if(strfind(string, "#", true) != -1) return 1;
	if(strfind(string, "\"", true) != -1) return 1;
	return 0;
}

